Add a new building

<%= form_for @building do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%=f.label :year_built %>
  <%=f.text_field :year_built %>

  <%= f.label :photo_url %>
  <%= f.url_field :photo_url %>
  <%= f.submit %>
<%end%>

<%=link_to("Back to all buildings", buildings_path) %>